  "license": "MIT",
  "type": "module",
  "dependencies": {
    "express": "^4.18.1",
    "lowdb": "^3.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/lowdb": "^1.0.11",
    "chai": "^4.3.6",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.20",
    "rollup": "^4.9.6",
    "rollup-plugin-typescript2": "^0.36.0",
    "supertest": "^6.2.4",
    "typescript": "^4.8.3"
  }
